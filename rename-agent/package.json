    "@types/node": "^18.0.0",
    "@types/sharp": "^0.31.0",
    "jest": "^27.0.0",
    "ts-jest": "^27.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": ["**/test/**/*.test.ts"],
    "globals": {
